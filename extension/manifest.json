{
    "name": "Interlope",
    "version": "1.0",
    "description": "control media with user input from beyond chrome",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
      "permissions": [
        "contextMenus"
      ],
      "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
      }
  }